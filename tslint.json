{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "member-access": false,
    "no-console": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "dist/**/*.js",
      "config/**/*.js",
      "scripts/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
