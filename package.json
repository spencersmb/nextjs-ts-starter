{
  "name": "et-shoppe-ts",
  "version": "1.0.0",
  "description": "Next.js Typescript Framework for etShoppe App",
  "scripts": {
    "deploy": "next build && next export",
    "dev": "run-p styleguide nextDev",
    "nextDev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "export": "next export",
    "styleguide": "styleguidist server --config styleguide.config.js",
    "styleguide:build": "styleguidist build --config styleguide.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "jest:test": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@et/products(.*)$": "<rootDir>/src/components/products$1"
    },
    "transform": {
      "^.+\\.jsx?$": "./node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/jest/typescriptTransform.js",
      "^.test.tsx?$": "<rootDir>/jest/jest-babel-plugin-module-resolver.js",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "setupFiles": [
      "<rootDir>/jest/polyfills.js",
      "<rootDir>/jest/setupTests.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupJest.js",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true,
        "tsConfigFile": "/Users/et64/CloudStation/github/et-shoppe-ts/tsconfig.test.json"
      }
    }
  },
  "author": "Spencer Bigum",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-typescript": "1.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "express": "^4.16.3",
    "form-data": "^2.3.2",
    "gsap": "^1.20.3",
    "isomorphic-unfetch": "^2.0.0",
    "next": "latest",
    "next-redux-wrapper": "latest",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.3",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "5.0.7",
    "react-redux-toastr": "^7.3.0",
    "react-stripe-elements": "^2.0.1",
    "react-transition-group": "^2.4.0",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.2",
    "redux-thunk": "2.2.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/jest": "^23.1.4",
    "@types/next": "latest",
    "@types/next-redux-wrapper": "^2.0.0",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/react-redux-toastr": "^7.1.0",
    "@types/react-stripe-elements": "^1.1.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-transition-group": "^2.0.11",
    "@types/sinon": "^5.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-styled-components": "^1.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "js-yaml": "^3.12.0",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.17.5",
    "react-docgen-typescript": "^1.6.1",
    "react-scripts-ts": "^2.16.0",
    "react-styleguidist": "^7.0.20",
    "react-test-renderer": "^16.4.1",
    "resize-observer-polyfill": "^1.5.0",
    "sinon": "^6.1.4",
    "strip-json-comments": "^2.0.1",
    "ts-jest": "22.0.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.9.2"
  }
}
